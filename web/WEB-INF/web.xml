<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>OutboundOrderCheckServlet</servlet-name>
        <servlet-class>com.sky.controller.OutboundOrderCheckServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckOrderServlet</servlet-name>
        <servlet-class>com.sky.controller.CheckOrderServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetOrderServlet</servlet-name>
        <servlet-class>com.sky.controller.GetOrderServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteOrderServlet</servlet-name>
        <servlet-class>com.sky.controller.DeleteOrderServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetAllUserServlet</servlet-name>
        <servlet-class>com.sky.controller.GetAllUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DisableLoginServlet</servlet-name>
        <servlet-class>com.sky.controller.DisableLoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AbleLoginServlet</servlet-name>
        <servlet-class>com.sky.controller.AbleLoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OrderCheckServlet</servlet-name>
        <servlet-class>com.sky.controller.OrderCheckServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>OrderCheckServlet</servlet-name>
        <url-pattern>/orderCheckServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AbleLoginServlet</servlet-name>
        <url-pattern>/ableLoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DisableLoginServlet</servlet-name>
        <url-pattern>/disableLoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetAllUserServlet</servlet-name>
        <url-pattern>/getAllUserServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteOrderServlet</servlet-name>
        <url-pattern>/deleteOrderServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetOrderServlet</servlet-name>
        <url-pattern>/getOrderServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckOrderServlet</servlet-name>
        <url-pattern>/checkOrderServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OutboundOrderCheckServlet</servlet-name>
        <url-pattern>/outboundOrderCheckServlet</url-pattern>
    </servlet-mapping>
</web-app>